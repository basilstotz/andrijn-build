#!/usr/bin/env xdg-open
[Desktop Entry]
Version=1.1
Name=Mille feuilles
Name[de]=Mille feuilles
GenericName=Mille feuilles
GenericName[de]=Mille feuilles
Comment=Französisch lernen
Comment[de]=Französisch lernen
Exec=/usr/share/bin/chromium.sh millefeuille/index.html
Terminal=false
X-MultipleArgs=false
Type=Application
Icon=mf3.png
Categories=Education;
StartupNotify=true
Actions=3.1;3.2;3.3;4.1;4.2;4.3;

[Desktop Action 3.1]
Exec=/usr/share/bin/chromium.sh millefeuille/3.1/START.html
Exec=/usr/bin/chromium-browser --app=$(/usr/share/bin/web-path)/
Name=Part   3.1
Icon=mf3.1.png

[Desktop Action 3.2]
Exec=/usr/share/bin/chromium.sh millefeuille/3.2/START.html
Name=Part   3.2
Icon=mf3.2.png

[Desktop Action 3.3]
Exec=/usr/share/bin/chromium.sh millefeuille/3.3/START.html
Name=Part   3.3
Icon=mf3.3.png

[Desktop Action 4.1]
Exec=/usr/share/bin/chromium.sh millefeuille/4.1/START.html
Name=Part   4.1
Icon=mf41.png

[Desktop Action 4.2]
Exec=/usr/share/bin/chromium.sh millefeuille/4.2/START.html
Name=Part   4.2
Icon=mf42.png

[Desktop Action 4.3]
Exec=/usr/share/bin/chromium.sh millefeuille/4.3/START.html
Name=Part    4.3
Icon=mf43.png

